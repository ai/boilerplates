{
  "name": "NAME",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/NAME",
  "sideEffects": false,
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/ai"
    }
  ],
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
    "test:lint": "eslint .",
    "test:coverage": "c8 pnpm unit",
    "test:types": "check-dts",
    "test:size": "size-limit",
    "test": "pnpm run /^test:/"
  },
  "devDependencies": {
    "@logux/eslint-config": "^52.0.0",
    "@size-limit/preset-small-lib": "^9.0.0",
    "c8": "^8.0.1",
    "check-dts": "^0.7.2",
    "clean-publish": "^4.2.0",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-perfectionist": "^2.0.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-prefer-node-builtin-imports": "^0.0.3",
    "eslint-plugin-promise": "^6.1.1",
    "size-limit": "^9.0.0",
    "tsm": "^2.3.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6"
  },
  "c8": {
    "exclude": [
      "**/*.test.*"
    ],
    "lines": 100,
    "reporter": "lcov",
    "check-coverage": true
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "eslintIgnore": [
    "**/errors.ts"
  ],
  "size-limit": [
    {
      "import": "{ }",
      "limit": "0 B"
    }
  ],
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "clean-publish": {
    "cleanDocs": true
  }
}
