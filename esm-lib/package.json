{
  "name": "NAME",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/NAME",
  "sideEffects": false,
  "engines": {
    "node": "^12.0.0 || ^14.0.0 || >=16.0.0"
  },
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
    "test": "c8 pnpm unit && eslint . && check-dts && size-limit"
  },
  "devDependencies": {
    "@logux/eslint-config": "^47.2.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "c8": "^7.11.2",
    "check-dts": "^0.6.7",
    "clean-publish": "^4.0.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "size-limit": "^7.0.8",
    "tsm": "^2.2.1",
    "typescript": "^4.6.4",
    "uvu": "^0.5.3"
  },
  "c8": {
    "exclude": [
      "**/*.test.*"
    ],
    "lines": 100,
    "reporter": "lcov",
    "check-coverage": true
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "eslintIgnore": [
    "**/errors.ts"
  ],
  "size-limit": [
    {
      "import": "{ }",
      "limit": "0 B"
    }
  ],
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "clean-publish": {
    "cleanDocs": true
  }
}
