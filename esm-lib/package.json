{
  "name": "NAME",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/NAME",
  "sideEffects": false,
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/ai"
    }
  ],
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test:lint": "eslint .",
    "test:coverage": "c8 pnpm bnt",
    "test:types": "check-dts",
    "test:size": "size-limit",
    "test": "pnpm run /^test:/"
  },
  "devDependencies": {
    "@logux/eslint-config": "^52.0.2",
    "@size-limit/preset-small-lib": "^11.0.2",
    "better-node-test": "^0.5.0",
    "c8": "^9.1.0",
    "check-dts": "^0.7.2",
    "clean-publish": "^4.2.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node-import": "^1.0.4",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "size-limit": "^11.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "c8": {
    "exclude": [
      "**/*.test.*",
      "test/*"
    ],
    "lines": 100,
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "skip-full": true,
    "clean": true
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "eslintIgnore": [
    "**/errors.ts"
  ],
  "size-limit": [
    {
      "import": "{ }",
      "limit": "0 B"
    }
  ],
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "clean-publish": {
    "cleanDocs": true
  }
}
